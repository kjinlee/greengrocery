package com.example.greengrocery.vegetable;

public class Vegetable {
	private String id;
	private String category;
	private String vegeName;
	private String price;

	public Vegetable() {
		
	}
	
	public Vegetable(String id, String category, String fruitName, String price) {
		super();
		this.id = id;
		this.category = category;
		this.vegeName = fruitName;
		this.price = price;
	}

	
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getFruitName() {
		return vegeName;
	}

	public void setFruitName(String fruitName) {
		this.vegeName = fruitName;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Vegetable [id=" + id + ", category=" + category + ", fruitName=" + vegeName + ", price=" + price + "]";
	}

}
