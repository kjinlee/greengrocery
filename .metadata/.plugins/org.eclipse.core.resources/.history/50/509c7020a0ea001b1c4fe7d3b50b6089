package com.example.greengrocery.fruit;

public class Fruit {
	private String id;
	private String category;
	private String fruitName;
	private String price;

	
	public Fruit() {
		
	}
	
	public Fruit(String id, String category, String fruitName, String price) {
		super();
		this.id = id;
		this.category = category;
		this.fruitName = fruitName;
		this.price = price;
	}

	public String getId() {
		return id;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getFruitName() {
		return fruitName;
	}

	public void setFruitName(String fruitName) {
		this.fruitName = fruitName;
	}

	
	@Override
	public String toString() {
		return "Fruit [id=" + id + ", category=" + category + ", fruitName=" + fruitName + ", price=" + price + "]";
	}
	
	

}
